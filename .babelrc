{
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      ["module-resolver", {
        "root": ["./"],
        "alias": {
          "test": "./test",
          "underscore": "lodash"
        }
      }],
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-transform-runtime"
    ],
    "sourceMaps": true,
    "retainLines": true
  }
  